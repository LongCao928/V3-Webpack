{
  "name": "v3-vuecli-project",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build-test": "vue-cli-service build --modern --mode test",
    "lint": "vue-cli-service lint",
    "lint-fix": "vue-cli-service lint  --fix --ext .vue,.js,.ts",
    "prepare": "husky install"
  },
  "dependencies": {
    "@element-plus/icons-vue": "^2.1.0",
    "axios": "^1.3.6",
    "core-js": "^3.8.3",
    "element-plus": "^2.3.3",
    "js-cookie": "^3.0.4",
    "lodash": "^4.17.21",
    "pinia": "^2.0.35",
    "qs": "^6.11.1",
    "vue": "^3.2.13",
    "vue-router": "^4.1.6"
  },
  "devDependencies": {
    "@babel/core": "^7.12.16",
    "@babel/eslint-parser": "^7.12.16",
    "@vue/cli-plugin-babel": "~5.0.0",
    "@vue/cli-plugin-eslint": "~5.0.0",
    "@vue/cli-service": "~5.0.0",
    "eslint": "^7.32.0",
    "eslint-plugin-vue": "^8.0.3",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.1",
    "path-browserify": "^1.0.1",
    "prettier": "^2.8.8",
    "sass": "^1.62.0",
    "sass-loader": "^13.2.2",
    "terser-webpack-plugin": "^5.3.7"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/vue3-essential",
      "eslint:recommended"
    ],
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    },
    "globals": {
      "_": true,
      "defineProps": "readonly",
      "defineEmits": "readonly"
    },
    "rules": {
      "vue/multi-word-component-names": "off"
    }
  },
  "lint-staged": { 
    "src/**/*.{js,vue,ts}": "npm run lint"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead",
    "not ie 11"
  ]
}
